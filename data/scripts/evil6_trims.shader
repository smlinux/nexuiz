{
	qer_editorimage textures/evil6_trims/e6metalfan.tga

	{
		map textures/evil6_trims/e6metalfan_blade.tga
		tcmod rotate 5000
	}
	{
		map textures/evil6_trims/e6metalfan.tga
		blendfunc blend
	}
	{
		map $lightmap
		blendfunc filter
