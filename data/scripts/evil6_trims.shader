textures/evil6_trims/e6btrim_light_s_nolight
{
	qer_editorimage textures/evil6_trims/e6btrim_light.tga
	surfaceparm nomarks

	{
		map textures/evil6_trims/e6btrim_light.tga
	}
	{
		map $lightmap
		blendfunc filter
	}
	{
		map textures/evil6_trims/e6btrim_light_blend.tga
		blendfunc add
	}
}

